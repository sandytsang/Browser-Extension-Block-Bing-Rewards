{
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*"
  },
  "mimeTypes": {
    ".crx": "application/x-chrome-extension"
  },
  "routes": [
    {
      "route": "/*.crx",
      "headers": {
        "Content-Type": "application/x-chrome-extension"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 404
    }
  }
}
